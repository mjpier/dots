#!/bin/bash

: "${BROWSER:=brave}"

config="$HOME/.config/dmscripts/config"

[[ -f "${config}" ]] && source "$config" 

if [[ ! "$(declare -p options 2>/dev/null)" == "declare -A"* ]] ; then 

  declare -A options
  options[Markdown Cheatsheet]="https://github.com/adam-p/markdown-here/wiki/Markdown-Cheatsheet"

fi
bookmarks=$(printf '%s\n' "${!options[@]}" | sort | dmenu -l 20 -p 'Bookmarks:') "$@" || exit

url="${options["${bookmarks}"]}"

${BROWSER} "${url}"
