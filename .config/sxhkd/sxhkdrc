XF86MonBrightness{Up,Down}
	brightnessctl s 10{+,-}

XF86Audio{Raise,Lower}Volume
	pamixer -{i,d} 10 --allow-boost

XF86AudioMute
	pamixer --toggle-mute

XF86AudioPlay
	playerctl play-pause

XF86AudioNext
	playerctl next

XF86AudioPrev
	playerctl previous

super + t
	alacritty

super + e
	thunar

super + w
	brave

super + r
	dmenu_run

Print
	flameshot gui

super + \
	~/.local/bin/list_of_dmenu_commands
