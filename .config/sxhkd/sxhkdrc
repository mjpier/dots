XF86AudioRaiseVolume
	pactl set-sink-volume @DEFAULT_SINK@ +10%

XF86AudioLowerVolume
	pactl set-sink-volume @DEFAULT_SINK@ -10%

XF86AudioMute
	pactl set-sink-mute @DEFAULT_SINK@ toggle

XF86AudioPlay
	playerctl play-pause

XF86AudioNext
	playerctl next

XF86AudioPrev
	playerctl previous

super + t
	alacritty

super + e
	thunar

super + w
	brave

super + r
	dmenu_run

Print
	flameshot gui

super + \
	~/.local/bin/dmemoji